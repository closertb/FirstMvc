<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.web.model.Users" table="users" schema="McGrady">
        <id name="id">
            <column name="id" sql-type="int(11) unsigned"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="pwd">
            <column name="pwd" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="updatedAt">
            <column name="updatedAt" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="createdAt">
            <column name="createdAt" sql-type="timestamp" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>